package practises;

public class Largest_Number {
	public static void main (String[]args) {
		int arr[] = {22,3,15,11,30,2};   
		int i=0;
		int max = arr[0];
		for (i = 1; i < arr.length; i++) {
			if (arr[i] > max) 
                max = arr[i]; 
       
			System.out.println("largest"+max); 
		}
			
	}
} 
			
			
			
				
			
			
			
			    
	



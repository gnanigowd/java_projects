package practises;

public class Largest_Number {
	public static void main (String[]args) {
		int arr[] = new int[] {22,3,15,11,30,2};   
		int i=0;
		int max = 0;
		while(max < arr.length) {
			if (i< arr[max]){
				i = arr[max];
			max=max+1;
		System.out.println(i);
			
		}
	}
			
			
			
				
			
			
			
			    
	}

}

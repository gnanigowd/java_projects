package mentor_questions;

import java.util.Scanner;

public class SUM {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("ENTER ANY NUMBER");
		int number = sc.nextInt();
		int sum = 0;
		for (int i = 0; i <= 10; i++) {
			sum = sum + i;
		}
		System.out.println(sum);
	}

}

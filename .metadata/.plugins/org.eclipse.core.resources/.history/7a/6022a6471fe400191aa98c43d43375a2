import java.util.Scanner;
public class factorial {
	public static void main(String[]args) {
		Scanner input = new Scanner(System.in);
		System.out.println("enter a number");
		int num = input.nextInt();
		int index=0;
		int fact=1;
		while (index <= num) {
			fact=fact*index;
			index=index+1;
		System.out.println(fact);
		}
	}

}

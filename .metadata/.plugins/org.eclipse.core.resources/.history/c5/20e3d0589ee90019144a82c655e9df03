import java.util.Scanner;

public class Count_Odd {
	public static void main(String[]args) {
		int n=0, i=0;
		Scanner sc = new Scanner(System.in);
		System.out.println("enter a number");
		int n = sc.nextInt();
		int i;
		for(i=1;i<n;i++)
		{
		if (i%2!=0);
			System.out.println(i+" ");
				
				
		}
	}
}

package mentor_questions;

import java.util.Scanner;

public class Store_Array {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int j[] = new int[10];
		for (int i = 0; i < j.length; i++) {
			System.out.println(j[i]);
			j[i] = sc.nextInt();

		}
		for (int i = 0; i < j.length; i++) {
			System.out.println(j[i]);
		}
	}
}

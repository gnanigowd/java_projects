package practice_que;

import java.util.Scanner;

public class Array_Sum {
	public static void main (String[]args) {
	int[] array = {10, 20, 30, 20, 20, 10};
    int sum = 0;
    for(  int num : array) {
        sum = sum + num;
    }
    System.out.println("Sum of array element:"+sum);
    	
    }
	
	
	}

package practice_java;

public class String_Question {
	public static void main(String[] args) {
		String var = new String("pragati");
		for (int i = 0; i < var.length(); i++) {
            char ch = var.charAt(i);
            if(ch == 'a' || ch == 'e' || ch == 'i'
                || ch == 'o' || ch == 'u') {
		
            	 System.out.print(ch);
            	 System.out.print(i);

		}

	}

}
}
